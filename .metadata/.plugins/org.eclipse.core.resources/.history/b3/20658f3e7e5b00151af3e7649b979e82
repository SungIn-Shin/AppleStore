package com.iheart.slkp.client;

public class Test {
	public static void main(String[] args) {
	byte[] header = new byte[4];
	String type ="CD";
	int len = 12;
	header = type.getBytes();
	System.out.println(header[0]);
	System.out.println(header[1]);
	
	header = SLKPUtil.intToByteArray(len);
	System.out.println(header[2]);
	System.out.println(header[3]);
	
	System.out.println(SLKPUtil.byteArrayToShort(header, 2));
	}
}
